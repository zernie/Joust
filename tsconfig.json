{
	"compilerOptions": {
		"declaration": true,
		"declarationDir": "dist",
		"jsx": "react",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"noLib": false,
		"removeComments": true,
		"sourceMap": true,
		"suppressImplicitAnyIndexErrors": true,
		"allowSyntheticDefaultImports": true,
		"target": "es6"
	},
	"include": ["ts"]
}
